{
  "name": "mimmi20/ios-build",
  "type": "library",
  "description": "Class wrapper for peterkahl/apple-ios-build-version",
  "homepage": "https://github.com/mimmi20/ios-build",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ios-build/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.7 || ^8.0.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "infection/infection": "^0.23.0",
    "mimmi20/coding-standard": "^2.2.2",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.89",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.20",
    "phpunit/phpunit": "^9.5.5"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IosBuild\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IosBuildTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ios-build/issues",
    "source": "https://github.com/mimmi20/ios-build"
  }
}
