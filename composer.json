{
  "name": "mimmi20/ios-build",
  "description": "Class wrapper for peterkahl/apple-ios-build-version",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ios-build/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ios-build",
  "support": {
    "issues": "https://github.com/mimmi20/ios-build/issues",
    "source": "https://github.com/mimmi20/ios-build"
  },
  "require": {
    "php": "~8.4.0 || ~8.5.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.31.9",
    "mimmi20/coding-standard": "^7.0.10",
    "nikic/php-parser": "^5.6.2",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.33",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpstan/phpstan-phpunit": "^2.0.9",
    "phpunit/phpunit": "^12.5.0",
    "rector/rector": "^2.2.11",
    "rector/type-perfect": "^2.1.1",
    "shipmonk/composer-dependency-analyser": "^1.8.4",
    "symfony/process": "^7.3.4",
    "symplify/phpstan-rules": "^14.9.3",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.1.0",
    "tomasvotruba/unused-public": "^2.1.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "IosBuild\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IosBuildTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
