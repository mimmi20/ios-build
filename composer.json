{
  "name": "mimmi20/ios-build",
  "description": "Class wrapper for peterkahl/apple-ios-build-version",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ios-build/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ios-build",
  "support": {
    "issues": "https://github.com/mimmi20/ios-build/issues",
    "source": "https://github.com/mimmi20/ios-build"
  },
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "mimmi20/coding-standard": "^2.6.4",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.5.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpunit/phpunit": "^9.5.19"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "IosBuild\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IosBuildTest\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
