{
  "name": "mimmi20/ios-build",
  "description": "Class wrapper for peterkahl/apple-ios-build-version",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ios-build/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ios-build",
  "support": {
    "issues": "https://github.com/mimmi20/ios-build/issues",
    "source": "https://github.com/mimmi20/ios-build"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11",
    "mimmi20/coding-standard": "^5.2.29",
    "nikic/php-parser": "^4.19.1",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.11.0",
    "phpstan/phpstan-deprecation-rules": "^1.2.0",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^10.5.20",
    "rector/rector": "^1.0.5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "IosBuild\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IosBuildTest\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
